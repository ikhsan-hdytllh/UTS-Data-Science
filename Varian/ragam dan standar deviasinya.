# Load dataset
data <- read.csv(file = 'D:\\Kuliah\\ABO\\Social_Network_Ads.csv') 

# Ambil kolom Age
kolom <- data$Age

# Varians (ragam)
varian <- var(kolom, na.rm = TRUE)

# Standar deviasi
standar_deviasi <- sd(kolom, na.rm = TRUE)

# Cetak hasil
cat("Varians (Ragam):", varian, "\n")
cat("Standar Deviasi:", standar_deviasi, "\n")
